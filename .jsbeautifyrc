{
    "mode": "beautify",
    "indent_size": 4,
    "indent_char": " ",
    "brace_style": "collapse,preserve-inline",
    "methodchain": "chain",
    "ternaryline": true,
    "formatObject": "default",
    "newline": true,
    "end_with_newline": true,
    "wrap": 120,
    "wrap_line_length": 120,
    "space_in_paren":false,
    "html": {
        "tagsort": true,
        "indent_size": 2,
        "js": {
            "indent_size": 2
        }
    },
    "js": {
        "preserve-newlines": true,
        "max_preserve_newlines": 2,
        "indent_size": 4
    }
}
